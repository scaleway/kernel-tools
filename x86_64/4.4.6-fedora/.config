# CONFIG_CMA_DEBUGFS is not set
CONFIG_CMA_AREAS=7

# Changes for persistent memory devices
# ZONE_DMA and ZONE_DEVICE can now co-exist
CONFIG_ZONE_DMA=y
CONFIG_ZONE_DEVICE=y
CONFIG_NVDIMM_PFN=y
CONFIG_ND_PFN=m

# Staging
CONFIG_STAGING_RDMA=y
